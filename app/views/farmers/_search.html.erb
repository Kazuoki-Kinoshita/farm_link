<%= search_form_for @q, url: url do |f| %>
  <%= f.label :name_or_product_or_address_cont, "キーワード" %>
  <%= f.search_field :name_or_product_or_address_cont %>

  <%= f.label :prefecture_id_eq, "検索エリア" %>
  <%= f.select :prefecture_id_eq, options_for_select(Prefecture.all.map { |p| [p.name, p.id] }, params[:q].try(:[], 'prefecture_id_eq')), { include_blank: 'すべて' } %>

  <%= f.submit "検索" %>
<% end %>